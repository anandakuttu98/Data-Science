Execute:
> select g.genre, count(n.title) as count_title
from genres as g
join netflix as n on g.genreid = n.genreid
group by g.genre
order by count_title

+ ---------- + ---------------- +
| genre      | count_title      |
+ ---------- + ---------------- +
| Supernatural | 1                |
| Historical | 2                |
| Mystery    | 3                |
| Satire     | 4                |
| Variety Show | 5                |
| Concert Film | 6                |
| Musical    | 9                |
| Interviews | 9                |
| Biopic     | 10               |
| Romance    | 14               |
| Science Fiction | 17               |
| Horror     | 19               |
| Action     | 20               |
| Anime      | 24               |
| Rom-Com    | 43               |
| Thriller   | 48               |
| Comedy     | 86               |
| Drama      | 105              |
| Documentary | 159              |
+ ---------- + ---------------- +
19 rows

Execute:
> select g.genre, count(n.title) as count_title
from genres as g
join netflix as n on g.genreid = n.genreid
where n.imdbscore > 8
group by g.genre
having count_title > 5
order by count_title

+ ---------- + ---------------- +
| genre      | count_title      |
+ ---------- + ---------------- +
| Documentary | 12               |
+ ---------- + ---------------- +
1 rows

Execute:
> select g.genre, avg(n.imdbscore) as avg_imdbscore, count(n.title) as count_title
from genres as g
inner join netflix as n on g.genreid = n.genreid
group by g.genre 
order by avg_imdbscore desc
limit 3

+ ---------- + ------------------ + ---------------- +
| genre      | avg_imdbscore      | count_title      |
+ ---------- + ------------------ + ---------------- +
| Concert Film | 7.633333333333333  | 6                |
| Historical | 7.050000000000001  | 2                |
| Documentary | 6.936477987421385  | 159              |
+ ---------- + ------------------ + ---------------- +
3 rows

